    // Editor
    "editor.detectIndentation": false, // Don't change indentation settings automatically
    "editor.insertSpaces": true, // Use spaces when hit tab key
    "editor.tabSize": 4,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.mouseWheelZoom": true, // Why this is off by default confounds me
    "editor.rulers": [100, 120, 140, 160, 180, 200],
    "editor.accessibilitySupport": "off",
    "editor.padding.bottom": 200,
    "editor.padding.top": 100,
    "editor.renderFinalNewline": "on",
    "editor.renderLineHighlight": "all",
    "editor.roundedSelection": false,
    "editor.scrollbar.verticalScrollbarSize": 8,
    "editor.scrollbar.horizontalScrollbarSize": 8,
    "editor.scrollBeyondLastColumn": 0,
    "editor.scrollBeyondLastLine": false,
    "editor.stickyScroll.enabled": true,
    "editor.stickyScroll.scrollWithEditor": false,
    "editor.wordWrapColumn": 120,
    "editor.formatOnSave": true,
    "editor.minimap.maxColumn": 80,
    "editor.minimap.renderCharacters": false,
    // Terminal
    "terminal.integrated.localEchoEnabled": "off",
    "terminal.integrated.allowChords": false,
    "terminal.integrated.altClickMovesCursor": false,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorStyleInactive": "line",
    "terminal.integrated.cursorWidth": 2,
    "terminal.integrated.defaultLocation": "editor",
    "terminal.integrated.drawBoldTextInBrightColors": false,
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "terminal.integrated.localEchoLatencyThreshold": -1,
    "terminal.integrated.minimumContrastRatio": 1,
    "terminal.integrated.scrollback": 100000,
    "terminal.integrated.shellIntegration.history": 1000,
    "terminal.integrated.shellIntegration.suggestEnabled": true,
    "terminal.integrated.stickyScroll.enabled": false, // Don't show past commands on top, its weird compared to a normal terminal
    "terminal.integrated.tabStopWidth": 4,
    // ETC
    "security.workspace.trust.enabled": false, // Ignore the trusted folders thing
    "telemetry.telemetryLevel": "off",
    "settingsSync.keybindingsPerPlatform": false,
    "files.associations": {
        "*.Ap1": "ssh_config",
        "*.include": "powershell",
        "*.simics": "powershell",
        "*.json": "jsonc",
        "*.css": "css"
    },
    "files.trimTrailingWhitespace": true, // Trim trailing spaces on save
    "diffEditor.maxFileSize": 100,
    "diffEditor.maxComputationTime": 8000,
    "window.autoDetectHighContrast": false,
    "window.density.editorTabHeight": "compact",
    "window.titleBarStyle": "custom",
    "update.mode": "start",
    "remote.downloadExtensionsLocally": true,
    "remote.SSH.configFile": "/home/cjc/.ssh/config",
    "remote.SSH.showLoginTerminal": true,
    "svelte.enable-ts-plugin": true,
    "svelte.plugin.svelte.compilerWarnings": {
        "a11y-click-events-have-key-events": "ignore",
        "a11y-no-static-element-interactions": "ignore",
        "a11y-no-noninteractive-element-interactions": "ignore"
    },
    "javascript.validate.enable": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "prettier.documentSelectors": ["*.{jsx,html}"],
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
